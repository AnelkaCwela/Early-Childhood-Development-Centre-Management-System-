@model SuburbModel
@{
    ViewData["Title"] = "Address";
}
<br />
<div class="card text-center">
    Centre Address
</div>
<br />
<form>
    <div class="form-group">
        <label asp-for="AddressLine1" class="control-label"></label>
        <input asp-for="AddressLine1" disabled class="form-control" />
        <span asp-validation-for="AddressLine1" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="AddressLine2" class="control-label"></label>
        <input asp-for="AddressLine2" disabled class="form-control" />
        <span asp-validation-for="AddressLine2" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="CityName" class="control-label"></label>
        <input asp-for="CityName" disabled class="form-control" />
        <span asp-validation-for="CityName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="PostalCode" class="control-label"></label>
        <input asp-for="PostalCode" disabled class="form-control" />
        <span asp-validation-for="PostalCode" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="RegionId" class="control-label"></label>
        <select asp-for="RegionId" disabled class="form-control" asp-items=" ViewBag.RegionId">
        </select>
        <span asp-validation-for="RegionId" class="text-danger"></span>
    </div>
</form>
